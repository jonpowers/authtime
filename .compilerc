{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["es2017-node7", "react"],
        "plugins": ["transform-async-to-generator", "transform-es2015-classes", "react-hot-loader/babel"],
        "sourceMaps": "inline"
      },
      "text/stylus" : {

      }
    },
    "production": {
      "application/javascript": {
        "presets": ["es2017-node7", "react"],
        "plugins": ["transform-async-to-generator", "transform-es2015-classes"],
        "sourceMaps": "none"
      },
      "text/stylus" : {

      }
    }
  }
}